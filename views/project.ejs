<%- include('partials/header', {username, position, title: 'Projects'}) %>

<div class="container">

  <h1>All Projects</h1>

  <table>
    <thead>
      <tr>
        <th>Key</th>
        <th>Summary</th>
        <th>Team Leader</th>
        <th>Status</th>
        <th>Created</th>
        <th>Time to resolution</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <% data.map(el => { %>
        <tr>
          <td>DEMO-<%= el.id %></td>
          <td><%= el.summary %></td>
          <td><%= el.leader %></td>
          <td><%= el.status %></td>
          <td><%= el.getDate() %></td>
          <td><%= convertTime(new Date(el.finished_time)) %></td>
          <td>
            <a href="/projects/edit/<%= el.id %> ">Edit</a>
            <a href="/">Show Location</a>
          </td>
        </tr>

      <% }) %>
    </tbody>
  </table>

</div>

<%- include('partials/footer') %>
