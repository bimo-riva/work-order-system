<%- include('partials/header') %>

<div class="container">

  <h1>EMPLOYEES</h1>

  <table>
    <thead>
      <tr>
        <td>Name</td>
        <td>Gender</td>
        <td>Position</td>
        <td>Username</td>
        <td>Email</td>
        <td>Password</td>
        <td>Status</td>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(el =>{ %> 
      <tr>
        <td><%= el.name %> </td>
        <td><%= el.gender %> </td>
        <td><%= el.position %> </td>
        <td><%= el.username %> </td>
        <td><%= el.email %> </td>
        <td><%= el.password %> </td>
        <td><%= el.status %> </td>
      </tr>
      <% }) %> 
    </tbody>
  </table>

</div>

<%- include('partials/footer') %>
